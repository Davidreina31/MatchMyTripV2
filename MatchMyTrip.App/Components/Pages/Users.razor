@page "/users"


<table class="table table-striped table-light">
    <thead>
        <tr>
            <th scope="col">Identifiant</th>
            <th scope="col">Prénom</th>
            <th scope="col">Nom</th>
            <th scope="col">Pseudo</th>
            <th scope="col">Role</th>
        </tr>
    </thead>
    <tbody>
        <div>
        </div>
        <tr>
            @foreach (var item in UserList)
            {
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.UserName</td>
                <td>@item.Role</td>
            }

            @* <td><input type="button" class="btn-success" value="modifier" (click)="updateRole(item.id)"></td>
            <td><input type="button" class="btn-danger" value="Supprimer" (click)="deleteUser(item.id)"></td> *@
        </tr>
    </tbody>
</table>
